{
  "name": "mybiblelog",
  "version": "1.0.0",
  "description": "A web app for tracking personal Bible reading, built with Nuxt.",
  "author": {
    "name": "Aaron Smith",
    "url": "https://github.com/AaronSmithX"
  },
  "private": true,
  "engines": {
    "node": "24.2.0"
  },
  "scripts": {
    "dev": "npm-run-all --parallel dev:api dev:nuxt",
    "dev:api": "cd api && npm run dev",
    "dev:nuxt": "cd nuxt && npm run dev",
    "start": "npm-run-all --parallel start:api start:nuxt",
    "start:api": "cd api && npm run start:prod",
    "start:nuxt": "cd nuxt && npm run start",
    "build": "cd nuxt && npm run build",
    "generate": "cd nuxt && npm run generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "test": "jest ./test shared",
    "test:serial": "jest --runInBand ./test shared",
    "test:watch": "jest --watch ./test shared",
    "test:api": "cd api && npm test",
    "test:e2e": "playwright test e2e",
    "test:e2e:ui": "playwright test e2e --ui",
    "script:seed": "cd api && npm run script:seed",
    "script:backup": "cd api && npm run script:backup",
    "script:delete-test-users": "cd api && npm run script:delete-test-users",
    "script:delete-stranded-data": "cd api && npm run script:delete-stranded-data"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "cross-env": "^7.0.3"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^5.0.0",
    "@nuxtjs/eslint-module": "^3.0.1",
    "@playwright/test": "^1.51.1",
    "@types/node": "^22.14.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-vue": "^7.20.0",
    "jest": "^29.7.0",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "openai": "^4.54.0",
    "supertest": "^7.1.0"
  }
}
